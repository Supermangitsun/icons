/**app.wxss**/
@font-face {
  font-family: 'icomoon';
  src:  url('https://supermangitsun.github.io/icons/fonts/icomoon.eot?r6jobo');
  src:  url('https://supermangitsun.github.io/icons/fonts/icomoon.eot?r6jobo#iefix') format('embedded-opentype'),
    url('https://supermangitsun.github.io/icons/fonts/icomoon.ttf?r6jobo') format('truetype'),
    url('https://supermangitsun.github.io/icons/fonts/icomoon.woff?r6jobo') format('woff'),
    url('https://supermangitsun.github.io/icons/fonts/icomoon.svg?r6jobo#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-add:before {
  content: "\e901";
}
.icon-delete:before {
  content: "\1f31a";
  color: #102e8e;
}
.icon-edit:before {
  content: "\e903";
  color: #303289;
}
.icon-preview:before {
  content: "\e900";
}
.icon-share:before {
  content: "\e904";
}
.icon-time:before {
  content: "\e905";
  color: #31337d;
}

